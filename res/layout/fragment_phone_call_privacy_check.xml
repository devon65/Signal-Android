                    android:layout_marginLeft="20dp"
                    style="@style/IdentityKey"
                    android:textColor="@color/gray78"
                    tools:text="956"/>

            </TableRow>

        </TableLayout>

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="75dp">

            <ImageView
                android:id="@+id/phone_call_match_button"
                android:layout_width="270dp"
                android:layout_height="match_parent"
                android:layout_marginRight="20dp"
                android:layout_weight="1"
                app:srcCompat="@drawable/ic_devon_privacy_check_phone_call_match_button" />

            <ImageView
                 android:id="@+id/phone_call_no_match_button"
                android:layout_width="270dp"
                android:layout_height="match_parent"
                android:layout_weight="1"
                app:srcCompat="@drawable/ic_devon_privacy_check_phone_call_no_match_button" />

        </LinearLayout>

    </LinearLayout>
<!--End Cyrus new-warning code-->
</FrameLayout>