<?xml version="1.0" encoding="utf-8"?>
<!--Cyrus new-warning code start-->
<FrameLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    tools:context=".PhoneCallPrivacyCheckFragment">


    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:paddingHorizontal="40dp"
        android:gravity="center_horizontal"
        android:orientation="vertical">

        <TextView
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:text="@string/phone_call_screen_Privacy_Check"
            android:textSize="20dp"
            android:gravity="center"
            android:textColor="@color/gray78"
            android:textStyle="bold"
            android:layout_margin="5dp"/>

        <TextView
            android:id="@+id/phone_call_these_are_the_device_identifiers"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:textSize="16dp"
            android:textColor="@color/gray78"
            android:text="@string/phone_call_screen_These_are_the_device_identifiers_for_you_and_s"
            android:justificationMode="inter_word"/>

        <TextView
            android:id="@+id/phone_call_have_friend_read_their_identifier"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:textSize="16dp"
            android:layout_marginVertical="10dp"
            android:textColor="@color/gray78"
            android:text="@string/phone_call_screen_Have_s_read_their_identifier_to_you"
            android:justificationMode="inter_word"/>

        <TextView
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:textSize="16dp"
            android:text="@string/phone_call_screen_Your_device_identifier"
            android:gravity="center"
            android:textColor="@color/gray78"
            android:textStyle="bold"/>
        <!--need to get code for fingerprint from this file: VerifyIdentityActivity, also has
        animation coded for me.-->

        <TableLayout android:id="@+id/my_number_table"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginTop="5dp"
            android:clickable="true"
            android:focusable="true">

            <TableRow android:gravity="center_horizontal"
                android:clickable="false"
                android:focusable="false">

                <TextView android:id="@+id/my_code_first"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    style="@style/IdentityKey"
                    android:textColor="@color/gray78"
                    tools:text="229"/>

                <TextView android:id="@+id/my_code_second"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginLeft="20dp"
                    style="@style/IdentityKey"
                    android:textColor="@color/gray78"
                    tools:text="569"/>

                <TextView android:id="@+id/my_code_third"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginLeft="20dp"
                    style="@style/IdentityKey"
                    android:textColor="@color/gray78"
                    tools:text="427"/>

                <TextView android:id="@+id/my_code_fourth"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginLeft="20dp"
                    style="@style/IdentityKey"
                    android:textColor="@color/gray78"
                    tools:text="200"/>

                <TextView android:id="@+id/my_code_fifth"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginLeft="20dp"
                    style="@style/IdentityKey"
                    android:textColor="@color/gray78"
                    tools:text="200"/>
            </TableRow>

            <TableRow android:gravity="center_horizontal">

                <TextView android:id="@+id/my_code_sixth"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    style="@style/IdentityKey"
                    android:textColor="@color/gray78"
                    tools:text="249"/>

                <TextView android:id="@+id/my_code_seventh"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginLeft="20dp"
                    style="@style/IdentityKey"
                    android:textColor="@color/gray78"
                    tools:text="585"/>

                <TextView android:id="@+id/my_code_eighth"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginLeft="20dp"
                    style="@style/IdentityKey"
                    android:textColor="@color/gray78"
                    tools:text="241"/>

                <TextView android:id="@+id/my_code_ninth"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginLeft="20dp"
                    style="@style/IdentityKey"
                    android:textColor="@color/gray78"
                    tools:text="002"/>

                <TextView android:id="@+id/my_code_tenth"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginLeft="20dp"
                    style="@style/IdentityKey"
                    android:textColor="@color/gray78"
                    tools:text="349"/>

            </TableRow>


        </TableLayout>

        <TextView
            android:id="@+id/phone_call_friend_device_identifier"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:textSize="16dp"
            android:text="@string/phone_call_screen_s_device_identifier"
            android:gravity="center"
            android:textColor="@color/gray78"
            android:textStyle="bold"
            android:layout_marginTop="15dp"/>

        <TableLayout android:id="@+id/thy_number_table"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginTop="5dp"
            android:clickable="true"
            android:focusable="true">

            <TableRow android:gravity="center_horizontal"
                android:clickable="false"
                android:focusable="false">

                <TextView android:id="@+id/thy_code_first"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    style="@style/IdentityKey"
                    android:textColor="@color/gray78"
                    tools:text="934"/>

                <TextView android:id="@+id/thy_code_second"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginLeft="20dp"
